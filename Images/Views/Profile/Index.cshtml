@model IEnumerable<Images.AspNetUser>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAll.cshtml";
}




@foreach (var item in Model)
{
    <br />
    <center>
        <div class="col-xl-3 col-lg-3 col-md-12 col-sm-12 col-12">
            <div class="card h-100">
                <div class="card-body">
                    <div class="account-settings">
                        <div class="user-profile">
                            <div class="user-avatar">
                                <img src=@Url.Content("~/Content/images/Profile/" + (item.LastName)) alt="Maxwell Admin" />
                            </div>
                            <h5 class="user-name">ชื่อ : @Html.DisplayFor(modelItem => item.FirstName)</h5>
                            <h6 class="user-email">อีเมล์ : @Html.DisplayFor(modelItem => item.Email)</h6>
                        </div>
                    </div>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Id }, new { @class = "btn btn-primary" })
                </div>
            </div>
        </div>
    </center>

}



